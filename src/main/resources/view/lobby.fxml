<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.shape.*?>
<?import java.net.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<VBox prefHeight="1200.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Region minHeight="40.0" />
      <HBox VBox.vgrow="ALWAYS">
         <children>
            <Region HBox.hgrow="ALWAYS" />
            <VBox alignment="TOP_CENTER" prefWidth="600.0">
               <children>
                  <HBox alignment="CENTER" styleClass="bg-primary">
                     <children>
                        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Players">
                           <styleClass>
                              <String fx:value="h2" />
                              <String fx:value="text-white" />
                              <String fx:value="b" />
                           </styleClass>
                        </Text>
                     </children>
                     <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </padding>
                  </HBox>
                  <VBox fx:id="playersTable" minHeight="90.0" styleClass="bg-light" VBox.vgrow="ALWAYS">
                     <padding>
                        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
                     </padding>
                     <children>
                     </children>
                  </VBox>
               </children>
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </VBox>
            <Region minWidth="40.0" HBox.hgrow="ALWAYS" />
            <VBox alignment="TOP_CENTER" prefWidth="600.0">
               <children>
                  <HBox alignment="CENTER" styleClass="bg-primary">
                     <children>
                        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Received challenges">
                           <styleClass>
                              <String fx:value="h2" />
                              <String fx:value="text-white" />
                              <String fx:value="b" />
                           </styleClass>
                        </Text>
                     </children>
                     <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </padding>
                  </HBox>
                  <VBox fx:id="challengesTable" minHeight="90.0" styleClass="bg-light">
                     <padding>
                        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
                     </padding>
                  </VBox>
               </children>
            </VBox>
            <Region HBox.hgrow="ALWAYS" />
         </children>
      </HBox>
      <Region minHeight="40.0" />
      <Separator prefWidth="200.0" />
      <Region minHeight="40.0" />
      <HBox alignment="CENTER">
         <children>
            <Button mnemonicParsing="false" onAction="#leave" text="Leave">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
               <padding>
                  <Insets bottom="6.0" left="10.0" right="10.0" top="6.0" />
               </padding>
               <styleClass>
                  <String fx:value="btn" />
                  <String fx:value="btn-game" />
               </styleClass>
            </Button>
         </children>
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </HBox>
      <Region minHeight="40.0" />
   </children>
   <stylesheets>
      <URL value="@../css/bootstrapfx.css" />
      <URL value="@../css/app.css" />
   </stylesheets>
</VBox>
